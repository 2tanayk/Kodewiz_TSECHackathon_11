<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/name23"
        android:layout_width="347dp"
        android:layout_height="81dp"
        android:layout_above="@+id/phone_Number"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="24dp"
        android:layout_marginRight="24dp"
        android:hint="Enter Name"
        android:visibility="visible" />

    <EditText
        android:id="@+id/customerId"
        android:layout_width="345dp"
        android:layout_height="84dp"
        android:layout_above="@+id/address"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="42dp"
        android:layout_marginRight="24dp"
        android:hint="id"
        android:visibility="visible" />

    <EditText
        android:id="@+id/fId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@id/button56"
        android:layout_below="@id/customerId"
        android:layout_alignTop="@+id/name23"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="23dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="262dp"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="94dp"
        android:hint="Feedbacl"
        android:visibility="visible" />

    <EditText

        android:id="@+id/feedback"
        android:layout_width="368dp"
        android:layout_height="95dp"
        android:layout_above="@+id/button56"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="24dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="209dp"
        android:hint="Enter Rating out of 5 :"
        android:inputType="number"
        android:visibility="visible" />

    <Button
        android:id="@+id/button56"
        android:layout_width="360dp"
        android:layout_height="75dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="15dp"
        android:layout_marginRight="24dp"
        android:layout_marginBottom="59dp"
        android:text="Submit" />
</RelativeLayout>
